<p id="notice"><%= notice %></p>

<h1>LISTAR ALUNOS</h1>

<table>
  <thead>
    <tr>
      <th>NOME</th>
      <th>SOBRENOME</th>
      <th>CPF</th>
      <th>ENDEREÇO</th>
      <th>PROFISSÃO</th>
      <th>E-MAIL</th>
      <th>CNH</th>
      <th>INSTITUIÇÃO</th>
      <th>CURSO</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @alunos.each do |aluno| %>
      <tr>
        <td><%= aluno.nome %></td>
        <td><%= aluno.sobrenome %></td>
        <td><%= aluno.cpf %></td>
        <td><%= aluno.endereco %></td>
        <td><%= aluno.profissao %></td>
        <td><%= aluno.email %></td>
        <td><%= aluno.cnh %></td>
        <td><%= aluno.instituicao %></td>
        <td><%= aluno.curso %></td>
        <td><%= link_to 'Show', aluno %></td>
        <td><%= link_to 'Edit', edit_aluno_path(aluno) %></td>
        <td><%= link_to 'Destroy', aluno, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'NOVO ALUNO', new_aluno_path %>
